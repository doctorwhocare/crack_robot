<launch>
 <!-- Turtlesim Node-->

 <!-- joy node -->
  <node respawn="true" pkg="joy"
        type="joy_node" name="turtle_joy" output="screen">
    <param name="dev" type="string" value="/dev/input/js0" />
    <param name="deadzone" value="0.12" />
  </node>
<!-- Axes -->
  <param name="axis_linear" value="1" type="int"/>
  <param name="axis_angular" value="0" type="int"/>
  <param name="scale_linear" value="2" type="double"/>
  <param name="scale_angular" value="2" type="double"/>
  <param name="LoadPath" value="/home/ru/model/save_human.txt" type="string"/>
  <param name="SavePath" value="/home/ru/model/save_human.txt" type="string"/>
  <node pkg="learning_joy" type="turtle_teleop_joy" name="teleop" output="screen"/>
</launch>
